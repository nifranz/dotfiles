            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            },
            "key_code": "semicolon"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "semicolon",
              "modifiers": [
              ]
            }
          ]
        }
      ]
    },
    {
