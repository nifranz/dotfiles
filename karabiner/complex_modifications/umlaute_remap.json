        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            },
            "key_code": "quote"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "quote",
              "modifiers": [
              ]
            }
          ]
        }
      ]
